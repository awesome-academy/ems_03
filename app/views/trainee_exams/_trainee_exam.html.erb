<tr>
  <td>
    <%= trainee_exam.exam_subject_name %>
  </td>
  <td>
    <%= link_to trainee_exam.exam_name,
      edit_trainee_exam_path(id: trainee_exam.id) %>
  </td>
  <td>
    <%= trainee_exam.user_name %>
  </td>
  <td>
    <%= l trainee_exam.created_at, format: :date_time %>
  </td>
   <td>
    <%= trainee_exam.total_score %>
  </td>
  <td>
    <% if trainee_exam.is_passed == nil %>
     <%= t ".not_marked" %>
    <% elsif trainee_exam.is_passed? %>
     <%= t ".passed" %>
    <% else %>
     <%= t ".not_passed" %>
    <% end %>
  </td>
</tr>
