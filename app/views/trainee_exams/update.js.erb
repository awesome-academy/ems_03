$("#score-<%=@trainee_exam.id%>").html(
  '<%= @trainee_exam.total_score %>');
$("#check-<%=@trainee_exam.id%>").html(
 '<% if @trainee_exam.is_passed == nil %>'
  + '<%= t ".not_marked" %>'
  + '<% elsif @trainee_exam.is_passed? %>'
  + '<%= t ".passed" %>'
  + '<% else %>'
  + '<%= t ".not_passed" %>'
  + '<% end %>');
$("#mark-<%=@trainee_exam.id%>").html(
  '<%= t ".marked" %>');
